{
  "name": "risco-cirurgico-desktop",
  "version": "0.2.0",
  "private": true,
  "author": "Matheus de Sousa",
  "description": "Risco Cirúrgico, ótimo para médicos autônomos.",
  "main": "public/electron.js",
  "homepage": "/",
  "build": {
    "appId": "RiscoCirurgico",
    "mac": {
      "category": "Medical"
    },
    "files": [
      "./public/electron.js"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.0.1",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.1.0",
    "styled-components": "^5.1.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "react-scripts start",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-dev": "concurrently \"BROWSER=none yarn dev\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-select": "^3.0.11",
    "concurrently": "^5.1.0",
    "electron": "^18.3.7",
    "electron-builder": "^22.5.1",
    "wait-on": "^4.0.2"
  }
}
